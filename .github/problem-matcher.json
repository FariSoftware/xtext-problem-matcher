{
  "problemMatcher": [
    {
      "owner": "xtext-validate",
      "pattern": [
        {
          "regexp": "^(.*):(.*)\\((.*)line\\s:\\s(\\d+)\\scolumn\\s:\\s(\\d+).*\\)$",
          "severity": 1,
          "message": 2,
          "file": 3,
          "line": 4,
          "column": 5
        }
      ]
    }
  ]
}
