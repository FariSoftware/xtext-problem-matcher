    "@zeit/ncc": "^0.22.3",
    "github-actions-problem-matcher-typings": "^1.1.0",
    "jest": "^25.5.4",
    "jest-circus": "^27.3.1",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.10"
  }
